{
  "name": "weather-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/app.js",
    "build": "npm run build:css && npm run build:js && npm run build:views",
    "build:css": "mkdir -p public/css && cp -R src/css public",
    "build:js": "mkdir -p public/js && cp -R src/js public",
    "build:views": "mkdir -p public && npx handlebars src/templates -f public/index.html -p src/templates/partials"
  },
